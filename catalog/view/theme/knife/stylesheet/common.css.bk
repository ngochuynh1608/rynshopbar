/* The "label", or whatever we should call it. Keep the first three lines for truncating. */
.sod_select .sod_label { display: block; overflow: hidden; white-space: nowrap; text-overflow: ellipsis; padding-right: 15px; }

.sod_select .sod_prefix { /* Use this if you're using a prefix and want to style it */ }

.sod_select .sod_placeholder { /* Use this if you're using a placeholder and want to style it */ }

/* Options list wrapper */
.sod_select .sod_list_wrapper { position: absolute; top: 100%; left: 0; display: none; height: auto; width: 200px; margin: 0 0 0 -3px; background: #ffffff; border: 3px solid #000000; border-top: none; color: #444444; font-weight: 300; z-index: 1; }

/* Shows the option list (don't edit) */
.sod_select.open .sod_list_wrapper { display: block; }

/* Don't display the options when  */
.sod_select.disabled.open .sod_list_wrapper { display: none; }

/* When the option list is displayed above the SoD */
.sod_select.above .sod_list_wrapper { top: auto; bottom: 100%; border-top: 3px solid #000000; border-bottom: none; }

/* Options list container */
.sod_select .sod_list { display: block; overflow-y: auto; padding: 0; margin: 0; }

/* All the options. Keep the first three lines for truncating... */
.sod_select .sod_option { display: block; overflow: hidden; white-space: nowrap; text-overflow: ellipsis; position: relative; padding: 10px 10px; list-style-type: none; }

/* Optgroups */
.sod_select .sod_option.optgroup, .sod_select .sod_option.optgroup.disabled { background: inherit; color: #939393; font-size: 10px; font-style: italic; }

/* Children of an optgroup */
.sod_select .sod_option.groupchild { padding-left: 20px; }

/* Disabled option */
.sod_select .sod_option.disabled { background: inherit; color: #cccccc; }

/* Hover state for options, also used when a user uses his/hers up/down keys */
.sod_select .sod_option.active { background: #f7f7f7; color: #333333; }

/*Make room for the check mark */
.sod_select .sod_option.selected { font-weight: 700; padding-right: 25px; }

/* Displays a check mark for the selected option */
.sod_select .sod_option.selected:before { content: ""; position: absolute; right: 10px; top: 50%; -webkit-transform: translateY(-50%); -ms-transform: translateY(-50%); transform: translateY(-50%); display: inline-block; color: #808080; height: 9px; width: 10px; background: url(data:image/svg+xml;base64,); }

/* Add a .no_highlight class to you SoD to hide the check mark and don't bold the option */
.sod_select.no_highlight .sod_option.selected { font-weight: 300; }

.sod_select.no_highlight .sod_option.selected:before { display: none; }

.sod_select .sod_option.link { /* If data-link is set on a specific option */ }

.sod_select .sod_option.linkexternal { /* If data-link-external is set on a specific option */ }

/* Hide native select */
.sod_select select { display: none !important; }

/* The native select in touch mode. Keep this first line. Sorry, keep everything. */
.sod_select.touch select { -webkit-appearance: menulist-button; position: absolute; top: 0; left: 0; display: block !important; height: 100%; width: 100%; opacity: 0; z-index: 1; }

/**************************************************
*
*	OWL CAROUSEL
*
***************************************************/
.owl-item > div { margin-right: 20px; }

.owl-carousel { width: calc(100% + 20px); }
.owl-carousel .owl-wrapper-outer { width: calc(100% - 20px); overflow: hidden; }

.owl-buttons > div { background-color: #b2b2b2; width: 17px; height: 17px; text-indent: -9999px; font-size: 0; line-height: 0; position: absolute; top: -35px; -moz-border-radius: 2px; -webkit-border-radius: 2px; border-radius: 2px; display: inline-block; vertical-align: middle; *vertical-align: auto; *zoom: 1; *display: inline; }
.owl-buttons > div:before, .owl-buttons > div:after { content: ""; position: absolute; width: 6px; height: 1px; background-color: #FFF; left: 50%; margin-left: -3px; }
.owl-buttons > div:before { top: 50%; margin-top: -3px; }
.owl-buttons > div:after { bottom: 50%; margin-bottom: -2px; }
.owl-buttons > div.owl-prev { right: 40px; }
.owl-buttons > div.owl-prev:before { -moz-transform: rotate(-45deg); -ms-transform: rotate(-45deg); -webkit-transform: rotate(-45deg); transform: rotate(-45deg); }
.owl-buttons > div.owl-prev:after { -moz-transform: rotate(45deg); -ms-transform: rotate(45deg); -webkit-transform: rotate(45deg); transform: rotate(45deg); }
.owl-buttons > div.owl-next { right: 20px; }
.owl-buttons > div.owl-next:before { -moz-transform: rotate(45deg); -ms-transform: rotate(45deg); -webkit-transform: rotate(45deg); transform: rotate(45deg); }
.owl-buttons > div.owl-next:after { -moz-transform: rotate(-45deg); -ms-transform: rotate(-45deg); -webkit-transform: rotate(-45deg); transform: rotate(-45deg); }
.owl-buttons > div:hover { background-color: #9b5ec3; }

.box-intro-member .owl-buttons > div { top: -50px; }

.box-intro-customer .owl-buttons > div { width: 26px; height: 26px; top: 50%; margin-top: -13px; }
.box-intro-customer .owl-buttons > div.owl-prev { right: auto; left: calc(-5% - 3px); }
.box-intro-customer .owl-buttons > div.owl-next { right: calc(-5% - 3px + 20px); }

/**************************************************
*
*	SELECT OR DIE
*
***************************************************/
.sod_select, .sod_select .sod_list_wrapper { width: 106px; margin: 0 10px; }

.sod_select { padding: 9px 10px; }

.sod_select, .sod_select .sod_list_wrapper { border: 1px solid #d8d8d8; }

.sod_select .sod_list_wrapper { margin: 0 0 0 -1px; }

.sod_select .sod_label { overflow: visible; }

.sod_select.focus { -moz-box-shadow: none; -webkit-box-shadow: none; box-shadow: none; }

.sod_select:hover, .sod_select.open, .sod_select.focus { border-color: #d8d8d8; cursor: pointer; }

.sod_select:before, .sod_select:after { content: ""; right: 5px; top: 6px; }

.sod_select:before { background-color: #adabab; width: 18px; height: 18px; }

.sod_select:after { border-left: 4px solid transparent; border-right: 4px solid transparent; border-top: 4px solid #fff; right: 10px; top: 13px; }

.sod_select .sod_option, .sod_select .sod_label { text-transform: none; }
